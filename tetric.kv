<MyGrid>:
    inputText:inputText.__self__

	GridLayout:
		cols:2
		size: root.width-20, root.height-20
		pos: 10,10

		GridLayout:
		    TextInput:
		        id:inputText
		        text:"Enter Text"

		GridLayout:
            Button:
				background_color:(0,0,1,1)
				text:"Run"
				on_press:convert()
